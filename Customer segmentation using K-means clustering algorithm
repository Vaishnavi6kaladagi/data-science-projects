            "text/plain": [
              "<Figure size 800x600 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.cluster import KMeans\n",
        "from sklearn.metrics import silhouette_score\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "# Generate synthetic dataset\n",
        "np.random.seed(42)\n",
        "n_samples = 100\n",
        "\n",
        "# Create a DataFrame with synthetic data\n",
        "data = {\n",
        "    'Purchase_Frequency': np.random.randint(1, 20, n_samples),  # Random purchase frequency between 1 and 20\n",
        "    'Amount_Spent': np.random.uniform(10, 500, n_samples),      # Random amount spent between 10 and 500\n",
        "    'Product_Types_Bought': np.random.randint(1, 5, n_samples)  # Random number of product types bought (1 to 4)\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# Display the first few rows of the dataset\n",
        "print(\"Synthetic Customer Data:\")\n",
        "print(df.head())\n",
        "\n",
        "# Standardize the data\n",
        "scaler = StandardScaler()\n",
        "scaled_data = scaler.fit_transform(df)\n",
        "\n",
        "# Determine the optimal number of clusters using the elbow method\n",
        "inertia = []\n",
        "silhouette_scores = []\n",
        "K_range = range(2, 11)\n",
        "\n",
        "for k in K_range:\n",
        "    kmeans = KMeans(n_clusters=k, random_state=42)\n",
        "    kmeans.fit(scaled_data)\n",
        "    inertia.append(kmeans.inertia_)\n",
        "    silhouette_scores.append(silhouette_score(scaled_data, kmeans.labels_))\n",
        "\n",
        "# Plot the elbow method\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.plot(K_range, inertia, marker='o')\n",
        "plt.title('Elbow Method')\n",
        "plt.xlabel('Number of Clusters (K)')\n",
        "plt.ylabel('Inertia')\n",
        "\n",
        "# Plot silhouette scores\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.plot(K_range, silhouette_scores, marker='o')\n",
        "plt.title('Silhouette Scores')\n",
        "plt.xlabel('Number of Clusters (K)')\n",
        "plt.ylabel('Silhouette Score')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# Choose the optimal number of clusters (for example, let's say we choose 3)\n",
        "optimal_k = 3\n",
        "kmeans = KMeans(n_clusters=optimal_k, random_state=42)\n",
        "df['Cluster'] = kmeans.fit_predict(scaled_data)\n",
        "\n",
        "# Display the clustered data\n",
        "print(\"\\nClustered Customer Data:\")\n",
        "print(df.head())\n",
        "\n",
        "# Display the cluster centers\n",
        "cluster_centers = scaler.inverse_transform(kmeans.cluster_centers_)\n",
        "print(\"\\nCluster Centers (in original scale):\")\n",
        "print(cluster_centers)\n",
        "\n",
        "# Visualize the clusters\n",
        "plt.figure(figsize=(8, 6))\n",
        "plt.scatter(df['Purchase_Frequency'], df['Amount_Spent'], c=df['Cluster'], cmap='viridis', marker='o')\n",
        "plt.scatter(cluster_centers[:, 0], cluster_centers[:, 1], c='red', marker='X', s=200, label='Centroids')\n",
        "plt.title('Customer Segmentation')\n",
        "plt.xlabel('Purchase Frequency')\n",
        "plt.ylabel('Amount Spent')\n",
        "plt.legend()\n",
        "plt.show()"
      ]
    }
  ]
}